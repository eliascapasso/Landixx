{
  "hosting": {
    "redirects": [
      {
        "source": "https://www.landixx.com/**",
        "destination": "https://landixx.com/:splat",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "/contact",
        "destination": "/contact.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}