{
  "hosting": {
    "redirects": [
      {
        "source": "https://www.landixx.com/**",
        "destination": "https://landixx.com/:splat",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "/contact",
        "destination": "/contact.html"
      },
      {
        "source": "/blog",
        "destination": "/blog/blog.html"
      },
      {
        "source": "/blog/website-redesign-signs",
        "destination": "/blog/website-redesign-signs.html"
      },
      {
        "source": "/blog/limitations-outdated-website",
        "destination": "/blog/limitation-outdated-website.html"
      },
      {
        "source": "/blog/adapt-embrace-digital-landscape",
        "destination": "/blog/adapt-embrace-digital-landscape.html"
      },
      {
        "source": "/blog/improve-website-ux-7-strategies",
        "destination": "/blog/improve-website-ux-7-strategies.html"
      },
      {
        "source": "/blog/need-online-presence",
        "destination": "/blog/need-online-presence.html"
      },
      {
        "source": "/blog/perfect-domain",
        "destination": "/blog/perfect-domain.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}