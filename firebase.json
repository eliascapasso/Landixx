{
  "hosting": {
    "redirects": [
      {
        "source": "https://www.landixx.com/**",
        "destination": "https://landixx.com/:splat",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/contact",
        "destination": "/contact.html"
      },
      {
        "source": "/blog",
        "destination": "/blog.html"
      },
      {
        "source": "/blog/website-redesign-signs",
        "destination": "/website-redesign-signs.html"
      },
      {
        "source": "/blog/limitations-of-outdated-website",
        "destination": "/article-2.html"
      },
      {
        "source": "/blog/adapt-embrace-digital-landscape",
        "destination": "/article-3.html"
      }
    ],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}